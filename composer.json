{
    "name": "template-interop/twig-adapter",
    "description": "Twig adapter for template-interop/engine",
    "keywords": [
        "render",
        "template",
        "engine",
        "templating",
        "output",
        "twig"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Keven",
            "email": "hello@keven.fr"
        }
    ],
    "require": {
        "php": ">=7.1",
        "template-interop/engine": "^0.0.2",
        "twig/twig": "^2.11"
    },
    "autoload": {
        "psr-4": {
            "Interop\\Template\\Twig\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3"
    },
    "autoload-dev": {
        "psr-4": {
            "Interop\\Template\\Twig\\": "src/"
        }
    }
}
